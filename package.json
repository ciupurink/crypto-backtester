{
  "name": "crypto-backtester",
  "version": "1.0.0",
  "description": "Cryptocurrency futures backtesting engine with Bybit data",
  "scripts": {
    "build": "tsc",
    "download": "ts-node src/cli.ts download",
    "backtest": "ts-node src/cli.ts backtest",
    "backtest:strategy-a": "ts-node src/cli.ts backtest --strategy ema-crossover",
    "backtest:strategy-b": "ts-node src/cli.ts backtest --strategy rsi-reversal",
    "backtest:strategy-c": "ts-node src/cli.ts backtest --strategy macd-volume",
    "backtest:strategy-d": "ts-node src/cli.ts backtest --strategy bollinger-squeeze",
    "backtest:strategy-e": "ts-node src/cli.ts backtest --strategy multi-timeframe",
    "backtest:strategy-f": "ts-node src/cli.ts backtest --strategy funding-rate",
    "backtest:strategy-h": "ts-node src/cli.ts backtest --strategy btc-accumulation",
    "backtest:strategy-i": "ts-node src/cli.ts backtest --strategy futures-rotation",
    "backtest:btc": "ts-node src/cli.ts backtest --symbol BTCUSDT",
    "backtest:eth": "ts-node src/cli.ts backtest --symbol ETHUSDT",
    "backtest:sol": "ts-node src/cli.ts backtest --symbol SOLUSDT",
    "dashboard": "ts-node src/dashboard.ts"
  },
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "typescript": "^5.3.2",
    "ts-node": "^10.9.2"
  }
}
